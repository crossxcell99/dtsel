{
  "name": "@dtsel/dtsel",
  "version": "v2.0.1",
  "description": "A simplistic pure javascript datetime selector",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "classic": "dist/dtsel.min.js",
  "types": "dist/types.d.ts",
  "targets": {
    "classic": {
      "context": "browser",
      "outputFormat": "global",
      "source": "src/startBrowser.ts",
      "optimize": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "main": {
      "source": "src/start.ts"
    },
    "module": {
      "source": "src/start.ts"
    },
    "types": {
      "source": "src/start.ts"
    }
  },
  "scripts": {
    "start": "parcel src/sample.html",
    "build": "parcel build",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crossxcell99/dtsel.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crossxcell99/dtsel/issues"
  },
  "homepage": "https://github.com/crossxcell99/dtsel#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@parcel/config-default": "^2.10.3",
    "@parcel/core": "^2.10.3",
    "@parcel/packager-ts": "2.10.3",
    "@parcel/transformer-js": "^2.10.3",
    "@parcel/transformer-react-refresh-wrap": "^2.10.3",
    "@parcel/transformer-typescript-types": "2.10.3",
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
